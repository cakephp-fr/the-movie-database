{
    "name": "ch0c4/TheMovieDatabase",
    "description": "TheMovieDatabase datasource plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords":[
        "cakephp",
        "plugin",
        "movie",
        "tv",
        "api",
        "tmdb"
    ],
    "homepage": "https://github.com/ch0c4/TMDB-Cakephp",
    "authors":[
        {
            "name": "Johan Sampedro",
            "role": "author",
            "homepage": "https://github.com/ch0c4"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "TheMovieDatabase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheMovieDatabase\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
